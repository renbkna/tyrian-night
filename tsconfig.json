{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2024",
    "lib": [
      "ES2024"
    ],
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "out",
    "strict": true,
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}
