  "semanticTokenColors": {
    "*.declaration": {
      "bold": true
    },
    "parameter": {
      "foreground": "#B068A0",
      "italic": true
    },
    "variable.local": {
      "foreground": "#D0C8E0"
    },
    "variable.defaultLibrary": {
      "foreground": "#C09040",
      "bold": true
    },
    "variable.readonly": {
      "foreground": "#C09040"
    },
    "property.readonly": {
      "foreground": "#C09040"
    },
    "function": {
      "foreground": "#3A9690",
      "italic": true
    },
    "function.defaultLibrary": {
      "foreground": "#3A9690"
    },
    "method": {
      "foreground": "#3A9690",
      "italic": true
    },
    "class": {
      "foreground": "#5A78C0"
    },
    "class.defaultLibrary": {
      "foreground": "#5A78C0"
    },
    "interface": {
      "foreground": "#5A78C0"
    },
    "type": {
      "foreground": "#5A78C0"
    },
    "typeParameter": {
      "foreground": "#5A78C0",
      "italic": true
    },
    "namespace": {
      "foreground": "#8B6ABD"
    },
    "enum": {
      "foreground": "#5A78C0"
    },
    "enumMember": {
      "foreground": "#C09040"
    },
    "property": {
      "foreground": "#D0C8E0"
    },
    "decorator": {
      "foreground": "#C09040",
      "italic": true
    },
    "macro": {
      "foreground": "#C09040"
    },
    "regexp": {
      "foreground": "#B068A0"
    }
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7880A0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#8890B0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.template", "punctuation.definition.string"],
      "settings": {
        "foreground": "#489060"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "entity.name.tag",
        "entity.name.tag.js",
        "entity.name.tag.css",
        "meta.import",
        "keyword.control.import"
      ],
      "settings": {
        "foreground": "#8B6ABD"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "keyword.other.unit",
        "variable.other.enummember",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#C09040"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.type.interface",
        "entity.name.type.class",
        "support.type",
        "support.class",
        "support.class.component",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#5A78C0"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "entity.name.method",
        "support.type.property-name.css",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#3A9690",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable", "support.constant.property-value.css"],
      "settings": {
        "foreground": "#D0C8E0"
      }
    },
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#8B6ABD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.parameter", "meta.parameter", "entity.other.attribute-name"],
      "settings": {
        "foreground": "#B068A0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string.regexp", "constant.character.escape"],
      "settings": {
        "foreground": "#B068A0"
      }
    },
    {
      "scope": ["variable.language", "entity.name.function.decorator"],
      "settings": {
        "foreground": "#C09040",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.other.constant",
        "meta.decorator",
        "punctuation.decorator",
        "markup.list"
      ],
      "settings": {
        "foreground": "#C09040"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#706888"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#8B6ABD",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#D0C8E0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#B068A0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#3A9690"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#5A78C0"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#489060",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#C06868"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#C09040",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
